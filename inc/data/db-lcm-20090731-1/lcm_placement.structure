CREATE TABLE `lcm_placement` (
  `id_placement` bigint(21) NOT NULL auto_increment,
  `id_room` bigint(21) NOT NULL default '0',
  `id_client` bigint(21) NOT NULL default '0',
  `date_start` datetime default NULL,
  `date_end` datetime default NULL,
  `status` enum('terminated','active','provisional','accepted','declined') default NULL,
  `id_case` bigint(21) NOT NULL default '0',
  PRIMARY KEY  (`id_placement`)
) ENGINE=MyISAM AUTO_INCREMENT=17 DEFAULT CHARSET=utf8